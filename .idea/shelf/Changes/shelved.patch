@@ -0,0 +1,27 @@
+{% extends 'cafe/base.html' %}
+
+{% block breadcrumbs %}
+{% with post.tags.all as tags %}
+{% if tags %}
+<ul class="tags_list">
+    <li>Тип Заведения:</li>
+    {% for t in tags %}
+    <li><a href="{{ t.get_absolute_url }}">{{ t.tag }}</a></li>
+    {% endfor %}
+    </ul>
+{% endif %}
+{% endwith %}
+{% endblock %}
+
+
+{% block content %}
+<p>Район: {{ post.cat.name }}</p>
+
+<h1>{{ post.title }}</h1>
+
+{% if post.photo %}
+<p><img class="img-article-left" src="{{ post.photo.url }}"></p>
+{% endif %}
+{{ post.content|linebreaks }}
+
+{% endblock %}
